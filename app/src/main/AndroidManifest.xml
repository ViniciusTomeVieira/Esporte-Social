<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vieira.vinny.cardview">

    <application
        android:allowBackup="true"
        android:icon="@drawable/esportesocial"
        android:label="@string/app_name"
        android:roundIcon="@drawable/esportesocial"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity
            android:name=".activity.EditarPerfilActivity"
            android:parentActivityName=".activity.MainActivity"></activity>
        <activity android:name=".activity.CriarTorneio"></activity>
    </application>

</manifest>